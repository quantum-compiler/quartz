#!/bin/bash

echo " Defaults for Linux machine"

# set CUQUANTUM dir
CUQUANTUM_DIR=/shared/cuquantum-linux-x86_64-22.11.0.13-archive

# set CUDA dir in case cmake cannot autodetect a path
CUDA_DIR=/usr/local/cuda-11.4

# set nccl dir
NCCL_DIR=/shared/nccl/build

# using simulator
QUARTZ_USE_SIMULATOR=ON

. $(dirname $0)/config.inc
run_cmake $*
